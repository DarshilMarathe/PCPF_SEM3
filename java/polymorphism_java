package com.learning;

// CALCULATOR FUNCTIONS
public class polymorphism
{
    public static void calculate(int a,int b)
    {
        int c = a+b;
        System.out.println("THE SUM OF TWO INT NUMBERS IS "+c);
    }
    public static void calculate(double a,int b) {
        double p = a*b;
        System.out.println("THE PRODUCT OF ONE DOUBLE AND OTHER INT NUMBER IS "+p);
    }
    public static void calculate(int a,double b){
        double diff = a-b;
        System.out.println("THE DIFFERENCE OF ONE INT AND OTHER DOUBLE NUMBER IS "+diff);
    }
    public static void calculate(double a,double b)
    {
        double q = a/b;
        System.out.println("THE QUOTIENT OF THE NUMBERS IS "+q);
    }

    public static void main(String[] args) {
        calculate(4,5);
        calculate(5.5,6);
        calculate(5.5,6.5);
        calculate(6,3.5);
    }
    
}
